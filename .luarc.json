{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "library": [
      "$VIM/runtime/lua",
      "./lua",
      "$XDG_DATA_HOME/nvim/lazy/plenary.nvim/lua",
      "$XDG_DATA_HOME/nvim/lazy/nui.nvim/lua"
    ],
    "checkThirdParty": false
  },
  "diagnostics": {
    "globals": ["vim"]
  }
}

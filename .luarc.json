{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME",
      "./lua",
      "${3rd}/luv/library",
      "${3rd}/plenary.nvim/lua"
    ],
    "checkThirdParty": false
  },
  "diagnostics": {
    "globals": ["vim"]
  }
}
